ts = reg && reg.defaults;\n      const options = {\n        ...defaults,\n        ...n.options,\n      };\n      return (\n        Component && (\n          <PiralModalsDialog {...n} options={options} defaults={reg.defaults} layout={reg.layout} key={n.name}>\n            <Component onClose={n.close} options={options} />\n          </PiralModalsDialog>\n        )\n      );\n    })\n    .filter(Boolean);\n  const open = children.length > 0;\n\n  return (\n    <PiralModalsHost open={open} close={close}>\n      {children}\n    </PiralModalsHost>\n  );\n};\nModals.displayName = 'Modals';\n"},"lineCount":null}},"error":null,"hash":"6bc20fb64ce765c1e46354d3dedc781c","cacheData":{"env":{}}}